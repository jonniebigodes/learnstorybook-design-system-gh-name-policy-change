{
  "name": "@jonniebigodes/learnstorybook-design-system-example",
  "version": "0.2.2",
  "description": "A Learn Storybook design system example",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "https://github.com/jonniebigodes/learnstorybook-design-system-gh-name-policy-change.git",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
